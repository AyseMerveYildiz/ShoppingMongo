@model List<ResultCategoryDto>
<div class="sec-banner bg0 p-t-80 p-b-50">
    <div class="container">
        <div class="p-b-10">
            <h3 class="ltext-103 cl5">
                Kategoriler
            </h3>
        </div>
        <br />
        <div class="row">
            <div class="col-md-6 col-xl-4 p-b-30 m-lr-auto">
              
                <div class="block1 wrap-pic-w">
                    <img src="/cozastore-master/images/gomlek01.jpg" alt="IMG-BANNER">

                    <a href="product.html" class="block1-txt ab-t-l s-full flex-col-l-sb p-lr-38 p-tb-34 trans-03 respon3">
                        <div class="block1-txt-child1 flex-col-l">
                            <span class="block1-name ltext-102 trans-04 p-b-8">
                                Gömlek
                            </span>

                            <span class="block1-info stext-102 trans-04">
                                Summer @DateTime.Now.Year
                            </span>
                        </div>

                        <div class="block1-txt-child2 p-b-4 trans-05">
                            <div class="block1-link stext-101 cl0 trans-09">
                                Hemen Satın Al
                            </div>
                        </div>
                    </a>
                </div>
            </div>

            @* <div class="col-md-6 col-xl-4 p-b-30 m-lr-auto">
               
                <div class="block1 wrap-pic-w">
                    <img src="/cozastore-master/images/jean01.jpg" alt="IMG-BANNER">

                    <a href="product.html" class="block1-txt ab-t-l s-full flex-col-l-sb p-lr-38 p-tb-34 trans-03 respon3">
                        <div class="block1-txt-child1 flex-col-l">
                            <span class="block1-name ltext-102 trans-04 p-b-8">
                                Jean
                            </span>

                            <span class="block1-info stext-102 trans-04">
                                Summer @DateTime.Now.Year
                            </span>
                        </div>

                        <div class="block1-txt-child2 p-b-4 trans-05">
                            <div class="block1-link stext-101 cl0 trans-09">
                                Hemen Satın Al
                            </div>
                        </div>
                    </a>
                </div>
            </div> *@
            @foreach (var item in Model)
            {
                <div class="col-md-6 col-xl-4 p-b-30 m-lr-auto">
                  
                    <div class="block1 wrap-pic-w">
                        <img src="@item.ImageUrl" alt="IMG-BANNER">

                        <a href="product.html" class="block1-txt ab-t-l s-full flex-col-l-sb p-lr-38 p-tb-34 trans-03 respon3">
                            <div class="block1-txt-child1 flex-col-l">
                                <span class="block1-name ltext-102 trans-04 p-b-8">
                                    @item.CategoryName
                                </span>

                                <span class="block1-info stext-102 trans-04">
                                    Summer @DateTime.Now.Year
                                </span>
                            </div>

                            <div class="block1-txt-child2 p-b-4 trans-05">
                                <div class="block1-link stext-101 cl0 trans-09">
                                    Hemen Satın Al
                                </div>
                            </div>
                        </a>
                    </div>
                </div>
            }
        </div>
    </div>
</div>